---
processes:
- name: blockhead
  executable: /var/vcap/packages/blockhead/bin/blockhead
  args:
  - /var/vcap/jobs/blockhead/config/blockhead-config.json
  - /var/vcap/jobs/blockhead/config/services
  env:
    DOCKER_HOST: unix:///var/vcap/data/sys/run/docker/docker.sock
  additional_volumes:
  - path: /var/vcap/data/sys/run/docker
